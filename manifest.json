{
  "manifest_version": 2,
  "name": "Nicolas Cage Extension",
  "version": "0.1.0",
  "description": "Nicolas Cage Extension",
  "icons": {
    "16": "cage.png",
    "48": "cage.png",
    "128": "cage.png"
  },
  "browser_action": {
    "default_icon": "cage.png"
},
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"]
  }]

}
